check process simple_webui
  with pidfile /var/vcap/sys/run/simple_webui/simple_webui.pid
  start program "/var/vcap/jobs/simple_webui/bin/simple_webui_ctl start"
  stop program "/var/vcap/jobs/simple_webui/bin/simple_webui_ctl stop"
  group vcap

check process router_registrar
  with pidfile /var/vcap/sys/run/router_registrar/router_registrar.pid
  start program "/var/vcap/jobs/simple_webui/bin/router_registrar_ctl start"
  stop program "/var/vcap/jobs/simple_webui/bin/router_registrar_ctl stop"
  group vcap